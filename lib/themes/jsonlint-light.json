{
  "name": "JSONLint Light",
  "type": "light",
  "colors": {
    "editor.background": "#ffffff",
    "editor.foreground": "#0f172a",
    "editor.lineHighlightBackground": "#f8fafc",
    "editorLineNumber.foreground": "#94a3b8",
    "editorLineNumber.activeForeground": "#475569",
    "editor.selectionBackground": "#3b82f630",
    "editorGutter.background": "#f8fafc"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#94a3b8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSON Property Keys",
      "scope": [
        "string.key",
        "support.type.property-name",
        "meta.object-literal.key",
        "string.json support.type.property-name.json",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#047857"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.quoted.double",
        "string.quoted.single",
        "string.value.json"
      ],
      "settings": {
        "foreground": "#2563eb"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.numeric.json"
      ],
      "settings": {
        "foreground": "#d97706"
      }
    },
    {
      "name": "Booleans and Null",
      "scope": [
        "constant.language",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.json"
      ],
      "settings": {
        "foreground": "#7c3aed"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.type"
      ],
      "settings": {
        "foreground": "#7c3aed"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#2563eb"
      }
    },
    {
      "name": "Classes and Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#047857"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#0f172a"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition.string",
        "punctuation.separator",
        "punctuation.definition.array",
        "punctuation.definition.dictionary",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#64748b"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#64748b"
      }
    },
    {
      "name": "Tags (HTML/XML)",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#dc2626"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#d97706"
      }
    }
  ]
}
