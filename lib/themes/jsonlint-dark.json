{
  "name": "JSONLint Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1a1d23",
    "editor.foreground": "#e4e4e7",
    "editor.lineHighlightBackground": "#22262e",
    "editorLineNumber.foreground": "#4b5563",
    "editorLineNumber.activeForeground": "#9ca3af",
    "editor.selectionBackground": "#3b82f640",
    "editorGutter.background": "#1a1d23"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#71717a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSON Property Keys",
      "scope": [
        "string.key",
        "support.type.property-name",
        "meta.object-literal.key",
        "string.json support.type.property-name.json",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#10b981"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.quoted.double",
        "string.quoted.single",
        "string.value.json"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.numeric.json"
      ],
      "settings": {
        "foreground": "#f59e0b"
      }
    },
    {
      "name": "Booleans and Null",
      "scope": [
        "constant.language",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.json"
      ],
      "settings": {
        "foreground": "#8b5cf6"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.type"
      ],
      "settings": {
        "foreground": "#8b5cf6"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Classes and Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#10b981"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#e4e4e7"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition.string",
        "punctuation.separator",
        "punctuation.definition.array",
        "punctuation.definition.dictionary",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#a1a1aa"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#a1a1aa"
      }
    },
    {
      "name": "Tags (HTML/XML)",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#ef4444"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#f59e0b"
      }
    }
  ]
}
